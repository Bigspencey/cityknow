<div class="container">
  <%= form_for [@city, @question] do |f| %>
  <div class="form-group">
    <%= f.label :title %><br/>
    <%= f.text_field :title, :size => "78" %>
  </div>
  <div class="form-group">
   <%= f.label :body  %><br/>
   <%= f.text_area :body, :cols => "80", :rows => "10" %>
 </div>
 <%= f.submit 'Submit', class: "btn btn-success" %>
 <% end %>
</div>
